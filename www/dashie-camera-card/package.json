{
  "name": "dashie-camera-card",
  "version": "1.0.0",
  "description": "Adaptive camera card for Home Assistant with platform-aware streaming",
  "main": "dist/dashie-camera-card.js",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "serve": "http-server dist -p 8080 --cors",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "home-assistant",
    "lovelace",
    "camera",
    "frigate",
    "webrtc",
    "hls"
  ],
  "author": "Dashie",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "http-server": "^14.1.1",
    "prettier": "^3.2.5",
    "rollup": "^4.9.6",
    "@rollup/plugin-terser": "^0.4.4",
    "typescript": "^5.3.3",
    "tslib": "^2.6.2"
  },
  "dependencies": {
    "hls.js": "^1.5.1",
    "lit": "^3.1.1"
  }
}
